{
  "build": {
    "beforeDevCommand": "pnpm dev",
    "beforeBuildCommand": "pnpm build",
    "frontendDist": "../dist",
    "devUrl": "http://localhost:1420"
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/stable/32x32.png",
      "icons/stable/128x128.png",
      "icons/stable/128x128@2x.png",
      "icons/stable/icon.icns",
      "icons/stable/icon.ico"
    ],
    "createUpdaterArtifacts": "v1Compatible"
  },
  "productName": "Overlayed",
  "version": "0.5.0",
  "identifier": "com.overlayed.dev",
  "plugins": {
    "updater": {
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IEM5MDVDNEVFQTc2QzJBNDYKUldSR0tteW43c1FGeVhzUm9vNEJKUkFyUXhSS2lrZWRuV1p1VWx5Tkg5bUhEWDZrOGprbGNnWkQK",
      "endpoints": [
        "https://update.overlayed.dev/{{target}}/{{arch}}/{{current_version}}"
      ]
    }
  },
  "app": {
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "transparent": true,
        "decorations": false,
        "shadow": false,
        "label": "main",
        "title": "Overlayed - Main",
        "width": 420,
        "height": 600,
        "minHeight": 106,
        "minWidth": 64
      },
      {
        "fullscreen": false,
        "transparent": true,
        "decorations": true,
        "resizable": true,
        "visible": false,
        "label": "settings",
        "url": "#settings",
        "title": "Overlayed - Settings",
        "width": 480,
        "height": 640
      }
    ],
    "withGlobalTauri": true,
    "macOSPrivateApi": true,
    "security": {
      "csp": null
    }
  }
}